{
    // THIS FILE NEEDS TO BE SAVED AS config.json AFTER YOU EDIT IT.
    // browserType: only "Chrome" currently fully supported, "Edge" may work needs more testing
    // browserProfile is optional and accepts the folder name of the browser profile you wish to use
    // if the folder name does not exist and a name is provided a new profile will be created
    // browserProfile defaults to your default profile if false
    "browserType": "Chrome",
    "broswerProfile": false,

    // debugLevel: "none" | "error" | "all"
    // dailyLog: if true will print out a status report in the ./logs folder after completion for the day
    // autoGamble: Automatically tries to meet play through requirements - NOT implemented.
    "debugLevel": "all",
    "dailyLog": true,
    "autoGamble": false,

    // Forever: If true will cause the bot to run in the background until stopped, collecting daily some time between the user specified interval
    // Collection time: Military time in the format ["hh:mm", "hh:mm"]
    "forever": false,
    "collectionTime": ["13:50", "14:30"],

    // Set the websites you have accounts with to be true, and the ones that you do not have as false
    "collect": {
        "chumba": true,
        "pulsz": true,
        "fortuneCoin": true,
        "luckyland": false,
        "wowVegas": true,
        "stake": false // NOT yet implemented
    },

    // Edit your usernames and passwords for the websites
    // If login is false standard password will be used instead
    // Logins need to be in the ["username", "password"] format ex ["johnsmith83", "badPassword14"]
    "logins": {
        "standard": ["username", "password"],
        "chumba": false,
        "pulsz": false,
        "fortuneCoin": false,
        "luckyLand": false,
        "wowVegas": false,
        "stake": false
    }
}